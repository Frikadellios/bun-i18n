{
  "name": "bun-i18n",
  "type": "module",
  "version": "1.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun astro check --verbose && bunx bun astro dev --verbose",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && bun astro check --verbose && bunx --bun astro build --verbose",
    "preview": "bunx --bun astro preview",
    "astro": "astro",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "translate:auto": "bunx @inlang/cli machine translate --project project.inlang"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.1.6",
    "@dmno/astro-integration": "^0.0.12",
    "@inlang/paraglide-astro": "^0.2.2",
    "@nanostores/i18n": "^0.12.2",
    "@nanostores/query": "^0.3.4",
    "@nanostores/react": "^0.7.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.14.2",
    "bun": "^1.1.24",
    "dmno": "^0.0.21",
    "i18n-type-safe": "^1.0.16",
    "i18next": "^23.13.0",
    "install": "^0.13.0",
    "nanostores": "^0.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "typesafe-i18n": "^5.26.2",
    "typescript": "^5.5.4",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@inlang/cli": "^2.18.1",
    "@inlang/sdk": "^0.36.3",
    "@types/bun": "latest",
    "@types/node": "^22.4.0",
    "@types/web": "^0.0.155",
    "@inlang/paraglide-js": "1.11.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "description": "template minimal",
  "keywords": [],
